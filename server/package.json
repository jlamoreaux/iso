{
  "name": "iso-app",
  "version": "1.0.0",
  "description": "ISO is a platform that helps photographers find and hire other photographers",
  "main": "dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "yarn build && node --es-module-specifier-resolution=node dist/app.js",
    "dev": "nodemon --watch src --exec 'tsc && node dist/app.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts-watch": "tsc -w --preserveWatchOutput",
    "watch-node": "nodemon --watch dist --exec 'node --es-module-specifier-resolution=node dist/app.js'",
    "build": "tsc",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix"
  },
  "repository": "git@github.com/jlamoreaux/iso",
  "author": "Jordan Lamoreaux",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.34",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "mongoose": "^6.8.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.2",
    "typescript": "^4.9.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.11.12",
    "@types/passport": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4"
  }
}
